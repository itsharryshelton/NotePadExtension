{
  "manifest_version": 2,
  "name": "Notepad",
  "version": "1.0",
  "description": "Notes",
  "permissions": ["storage"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "background": {
    "scripts": ["background.js"]
  }
}
